<!DOCTYPE html>
<html>
<head>
    #set($pageTitle = $i18n.get("title.replay.result") + " - " + $!{record.identity})
    #parse("blocks/html-head.vm")
    <!-- ace -->
    <link rel="stylesheet" href="/plugins/ace-diff/ace-diff.min.css">
</head>
<body class="hold-transition sidebar-mini">
<!-- Site wrapper -->
<div class="wrapper">
    #parse("blocks/navbar.vm")

    #set($currentPage = "online")
    #parse("blocks/sidebar.vm")

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1>$i18n.get("title.replay.result")</h1>
                    </div>
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="/online/detail.htm?appName=${record.appName}&traceId=$!{record.TraceId}">$i18n.get("breadcrumb.call.detail")</a></li>
                            <li class="breadcrumb-item active">$i18n.get("breadcrumb.replay.result")</li>
                        </ol>
                    </div>
                </div>
            </div><!-- /.container-fluid -->
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-12">
                    <div class="card card-blue card-outline">
                        <div class="card-header border-0">
                            <h5 class="card-title text-blue">
                                $i18n.get("page.replay.snapshot")
                            </h5>
                            <div class="card-tools">
                            </div>
                        </div>
                        <div class="card-body" style="padding: 5px">
                            <div class="col-lg-12 col-xs-12">
                                <ul class="nav nav-pills ml-auto">
                                    <li class="nav-item"><a class="nav-link active" href="#basic" data-toggle="tab" aria-expanded="false">$i18n.get("page.basic.info")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#request" data-toggle="tab" aria-expanded="false">$i18n.get("page.request.params")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#response" data-toggle="tab" aria-expanded="false">$i18n.get("page.response.result")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#subInvocations" data-toggle="tab" aria-expanded="false">$i18n.get("page.sub.invocations")</a></li>
                                </ul>
                                <div class="tab-content no-padding no-border" style="margin-top: 2px">
                                    <div class="tab-pane active" id="basic">
                                        <div class="col-xs-12">
                                            <table class="table table-hover table-striped" style="word-break: break-all;">
                                                <tbody>
                                                <tr>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("table.app.name")</td>
                                                    <td>$!record.appName</td>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("table.entrance")</td>
                                                    <td>$!record.entranceDesc</td>
                                                </tr>
                                                <tr>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("page.record.machine")</td>
                                                    <td>$!record.host</td>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("table.trace.id")</td>
                                                    <td>$!{record.traceId}</td>
                                                </tr>
                                                <tr>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("page.record.time")</td>
                                                    <td>$!record.gmtRecord</td>
                                                    <td width="100px" class="text-right text-bold">$i18n.get("page.record.environment")</td>
                                                    <td><span class="label label-primary">$!record.environment</span></td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                    <div class="tab-pane" id="request">
                                        <div class="col-xs-12" id="request-editor" style="height: 400px;display: none">$!record.request</div>
                                    </div>
                                    <div class="tab-pane" id="response">
                                        <div class="col-xs-12" id="response-editor" style="height: 400px;display: none">$!record.response</div>
                                    </div>
                                    <div class="tab-pane" id="subInvocations">
                                        <div class="col-xs-12" id="subInvocations-editor" style="height: 400px;display: none">$!record.subInvocations</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-footer clearfix" >
                        </div>
                    </div>
                </div>
                <div class="col-12">
                    <div class="card card-blue card-outline">
                        <div class="card-header border-0">
                            <h5 class="card-title text-blue">
                                $i18n.get("page.replay.result")
                            </h5>
                            <div class="card-tools">
                            </div>
                        </div>
                        <div class="card-body" style="padding: 5px">
                            <div class="col-lg-12 col-xs-12">
                                <ul class="nav nav-pills ml-auto">
                                    <li class="nav-item"><a class="nav-link active" href="#basic-replay" data-toggle="tab" aria-expanded="false">$i18n.get("page.basic.info")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#diffNode" data-toggle="tab" aria-expanded="false">$i18n.get("page.diff.nodes")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#resultDiff" data-toggle="tab" aria-expanded="false">$i18n.get("page.result.diff")</a></li>
                                    <li class="nav-item"><a class="nav-link" href="#mockProcess" data-toggle="tab" aria-expanded="false">$i18n.get("page.mock.process")</a></li>
                                </ul>
                                <div class="tab-content no-padding no-border" style="margin-top: 2px">
                                    <div class="tab-pane active" id="basic-replay">
                                        <div class="col-xs-12">
                                            <div class="col-xs-12">
                                                <table class="table table-hover table-striped" style="word-break: break-all;">
                                                    <thead>
                                                    <tr>
                                                        <th>$i18n.get("table.execution.result")</th>
                                                        <th>$i18n.get("table.execution.time")</th>
                                                        <th>$i18n.get("table.execution.environment")</th>
                                                        <th>$i18n.get("table.execution.machine")</th>
                                                        <th>$i18n.get("table.repeat.id")</th>
                                                        <th>$i18n.get("table.trace.id")</th>
                                                        <th>$i18n.get("table.execution.datetime")</th>
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                    <tr>
                                                        <td>
                                                            #if($!replay.status.desc == '执行中')
                                                                <span class="badge bg-primary">$i18n.get("status.executing")</span>
                                                            #else
                                                                #if($!replay.success)
                                                                        <span class="badge bg-success">$i18n.get("status.success")</span>
                                                                    #else
                                                                        <span class="badge bg-danger">$i18n.get("status.failed")</span>
                                                                #end
                                                            #end
                                                        </td>
                                                        <td>$!{replay.cost}ms</td>
                                                        <td>$!{replay.environment}</td>
                                                        <td>$!{replay.ip}</td>
                                                        <td>$!{replay.repeatId}</td>
                                                        <td>$!{replay.traceId}</td>
                                                        <td>$!replay.gmtCreate</td>
                                                    </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="tab-pane" id="diffNode">
                                        #if($!replay.differences && $!replay.differences.size()>0)
                                            <div class="col-xs-12 table-responsive no-padding">
                                                <table class="table table-bordered table-striped" style="word-break: break-all;">
                                                    <thead>
                                                    <tr>
                                                        <th style="min-width: 100px;">$i18n.get("table.node")</th>
                                                        <th style="min-width: 100px;">$i18n.get("table.expected.value")</th>
                                                        <th style="min-width: 100px;">$i18n.get("table.actual.value")</th>
                                                        <th style="min-width: 100px;">$i18n.get("table.reason")</th>
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                        #foreach($diff in $replay.differences)
                                                        <tr>
                                                            <td>$!diff.nodeName</td>
                                                            <td>
                                                                #if($diff.expect)
                                                                   $!diff.expect
                                                                #else
                                                                    <span class="text-purple">null</span>
                                                                #end
                                                            </td>
                                                            <td>
                                                                #if($diff.actual)
                                                                    $!diff.actual
                                                                #else
                                                                    <span class="text-purple">null</span>
                                                                #end
                                                            </td>
                                                            <td>$!diff.type</td>
                                                        </tr>
                                                        #end
                                                    </tbody>
                                                </table>
                                            </div>
                                        #else
                                            <h5 style="padding: 20px">$i18n.get("msg.no.diff.nodes")</h5>
                                        #end
                                    </div>
                                    <div class="tab-pane" id="resultDiff">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <span class="badge badge-success">$i18n.get("page.expected.result")</span>
                                            </div>
                                            <div class="col-md-6">
                                                <span class="badge badge-success" style="margin-left: 20px">$i18n.get("page.actual.result")</span>
                                            </div>
                                        </div>
                                        <div class="col-12">
                                            <div id="diff-patch-view" style="height: 450px"></div>
                                        </div>
                                        <div id="diff-path-left" style="display: none">$!{record.response}</div>
                                        <div id="diff-path-right" style="display: none">$!{replay.response}</div>

                                    </div>
                                    <div class="tab-pane" id="mockProcess">
                                        #if($!replay.mockInvocations && $!replay.mockInvocations.size()>0)
                                            <div class="col-xs-12 table-responsive no-padding">
                                                <table class="table table-hover table-striped" style="word-break: break-all;">
                                                    <thead>
                                                    <tr>
                                                        <th width="5%">$i18n.get("table.sequence")</th>
                                                        <th width="15%">$i18n.get("table.record.identity")</th>
                                                        <th width="15%">$i18n.get("table.replay.identity")</th>
                                                        <th width="27%">$i18n.get("table.record.params")</th>
                                                        <th width="27%">$i18n.get("table.replay.params")</th>
                                                        <th width="6%">$i18n.get("table.cost")</th>
                                                        <th width="5%">$i18n.get("table.status")</th>
                                                    </tr>
                                                    </thead>
                                                    <tbody>
                                                        #foreach($mock in $!replay.mockInvocations)
                                                        <tr>
                                                            <td>$!mock.index</td>
                                                            <td>$!mock.originUri</td>
                                                            <td>$!mock.currentUri</td>
                                                            <td>$!mock.originArgs</td>
                                                            <td>$!mock.currentArgs</td>
                                                            <td>$!{mock.cost}ms</td>
                                                            <td>
                                                                #if($!mock.skip)
                                                                    <span class="badge badge-warning">$i18n.get("table.skip")</span>
                                                                #elseif($!mock.success)
                                                                    <span class="badge badge-success">$i18n.get("status.success")</span>
                                                                #else
                                                                    <span class="badge badge-danger">$i18n.get("status.failed")</span>
                                                                #end
                                                            </td>
                                                        </tr>
                                                        #end
                                                    </tbody>
                                                </table>
                                            </div>
                                        #else
                                            <h5 style="padding: 20px">$i18n.get("msg.no.mock.process")</h5>
                                        #end
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-footer clearfix" >
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    #parse("blocks/footer.vm")
</div>
<!-- ./wrapper -->

#parse("blocks/header-scripts.vm")
<!-- ace -->
<script src="/plugins/ace-editor/ace.js" charset="UTF-8"></script>
<script src="/plugins/ace-diff/ace-diff.min.js" charset="UTF-8"></script>
#parse("blocks/fakeloader.vm")

<script src="/app/js/start-repaly.js" charset="UTF-8"></script>
<script type="application/javascript">
    jQuery(function($){
        if (document.getElementById("request-editor")) {
            var requestEditor = ace.edit("request-editor");
            requestEditor.setTheme("ace/theme/xcode");
            requestEditor.getSession().setMode("ace/mode/json");
            requestEditor.setReadOnly(true);
            requestEditor.setShowPrintMargin(false);
            requestEditor.resize();
            $('#request-editor').fadeIn(500);
        }

        if (document.getElementById("response-editor")) {
            var responseEditor = ace.edit("response-editor");
            responseEditor.setTheme("ace/theme/xcode");
            responseEditor.getSession().setMode("ace/mode/json");
            responseEditor.setReadOnly(true);
            responseEditor.setShowPrintMargin(false);
            responseEditor.resize();
            $('#response-editor').fadeIn(500);
        }
        if (document.getElementById("subInvocations-editor")) {
            var subInvocationsEditor = ace.edit("subInvocations-editor");
            subInvocationsEditor.setTheme("ace/theme/xcode");
            subInvocationsEditor.getSession().setMode("ace/mode/json");
            subInvocationsEditor.setReadOnly(true);
            subInvocationsEditor.setShowPrintMargin(false);
            subInvocationsEditor.resize();
            $('#subInvocations-editor').fadeIn(500);
        }
        if (document.getElementById("diff-path-left")) {
            var differ = new AceDiff({
                theme: "ace/theme/github",
                mode: "ace/mode/json",
                element: '#diff-patch-view',
                left: {
                    content: $("#diff-path-left").text(),
                    editable: false,
                    copyLinkEnabled: false
                },
                right: {
                    content: $("#diff-path-right").text(),
                    editable: false,
                    copyLinkEnabled: false
                },
            });
        }
    });
</script>
    #parse("blocks/start-replay.vm")
</body>
</html>
